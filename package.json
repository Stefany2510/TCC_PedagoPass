{
  "name": "pedagopass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:backend": "ts-node --project tsconfig.backend.json src/backend/server.ts",
    "build:backend": "tsc --project tsconfig.backend.json",
    "start:backend": "node dist/backend/server.js",
    "backend": "npm run dev:backend",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend\" --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,magenta\""
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "next": "^15.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.2.1",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2"
  }
}
